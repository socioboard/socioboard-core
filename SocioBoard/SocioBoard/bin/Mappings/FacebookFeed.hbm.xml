<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="SocioBoard.Domain"
      assembly="SocioBoard">

  <class name="FacebookFeed" table="FacebookFeed" lazy="false">
    <id name="Id" column="Id">
      <generator class="guid"/>
    </id>
    <property name="FeedDescription">
      <column name="FeedDescription" />
    </property>
    <property name="FeedDate">
      <column name="FeedDate"  />
    </property>
    <property name="EntryDate">
      <column name="EntryDate"  />
    </property>
    <property name="ProfileId">
      <column name="ProfileId"  />
    </property>


    <property name="FromId">
      <column name="FromId"  />
    </property>

    <property name="FromName">
      <column name="FromName"  />
    </property>

    <property name="FromProfileUrl">
      <column name="FromProfileUrl"  />
    </property>
    <property name="UserId">
      <column name="UserId"  />
    </property>
    <property name="Type">
      <column name="Type"  />
    </property>
    <property name="FbComment">
      <column name="FbComment"  />
    </property>
    <property name="FbLike">
      <column name="FbLike"  />
    </property>
    <property name="FeedId">
      <column name="FeedId"  />
    </property>

    <property name="ReadStatus">
      <column name="ReadStatus"  />
    </property>


  </class>
</hibernate-mapping>
