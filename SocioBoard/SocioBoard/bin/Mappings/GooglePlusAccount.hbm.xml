<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="SocioBoard.Domain"
      assembly="SocioBoard">

  <class name="GooglePlusAccount" table="GooglePlusAccount" lazy="false">
    <id name="Id" column="Id" >
      <generator class="guid"/>
    </id>
    <property name="UserId">
      <column name="UserId" />
    </property>
    <property name="GpUserId">
      <column name="GpUserId"  />
    </property>
    <property name="GpUserName">
      <column name="GpUserName"  />
    </property>
    <property name="GpProfileImage">
      <column name="GpProfileImage"  />
    </property>
    <property name="RefreshToken">
      <column name="RefreshToken"  />
    </property>
    <property name="AccessToken">
      <column name="AccessToken"  />
    </property>
    <property name="EmailId">
      <column name="EmailId"  />
    </property>   
    <property name="PeopleCount">
      <column name="PeopleCount" />
    </property>
    <property name="IsActive">
      <column name="IsActive"  />
    </property> 
    <property name="EntryDate">
      <column name="EntryDate" />
    </property>   
  </class>
</hibernate-mapping>