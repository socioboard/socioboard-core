<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="SocioBoard.Domain"
      assembly="SocioBoard">

  <class name="Team" table="Team" lazy="false">
    <id name="Id" column="Id">
      <generator class="guid"/>
    </id>
    <property name="UserId">
      <column name="UserId" />
    </property>

    <property name="GroupId">
      <column name="GroupId" />
    </property>
    
    <property name="EmailId">
      <column name="EmailId"  />
    </property>
    <property name="FirstName">
      <column name="FirstName"  />
    </property>
    <property name="LastName">
      <column name="LastName"  />
    </property>


    <property name="InviteDate">
      <column name="InviteDate"  />
    </property>

    <property name="StatusUpdateDate">
      <column name="StatusUpdateDate"  />
    </property>

    <property name="InviteStatus">
      <column name="InviteStatus"  />
    </property>
    <property name="AccessLevel">
      <column name="AccessLevel"  />
    </property>

  </class>
</hibernate-mapping>