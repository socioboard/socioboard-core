<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="SocioBoard.Domain"
      assembly="SocioBoard">

  <class name="FacebookAccount" table="FacebookAccount" lazy="false">
    <id name="Id" column="Id" >
      <generator class="guid"/>
    </id>
    <property name="FbUserId">
      <column name="FbUserId" />
    </property>
    <property name="EmailId">
      <column name="EmailId"  />
    </property>
    <property name="AccessToken">
      <column name="AccessToken"  />
    </property>
    <property name="Type">
      <column name="Type"  />
    </property>
    <property name="ProfileUrl">
      <column name="ProfileUrl"  />
    </property>
    
    <property name="UserId">
      <column name="UserId"  />
    </property>

    <property name="IsActive">
      <column name="IsActive"  />
    </property>

    <property name="Friends">
      <column name="Friends"  />
    </property>

    <property name="FbUserName">
      <column name="FbUserName"  />
    </property>
   
  </class>
</hibernate-mapping>