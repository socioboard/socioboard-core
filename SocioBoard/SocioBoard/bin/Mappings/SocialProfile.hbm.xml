<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="SocioBoard.Domain"
      assembly="SocioBoard">

  <class name="SocialProfile" table="SocialProfile" lazy="false">
    <id name="Id" column="Id">
      <generator class="guid"/>
    </id>
    <property name="UserId">
      <column name="UserId" />
    </property>
    <property name="ProfileId">
      <column name="ProfileId"  />
    </property>
    <property name="ProfileDate">
      <column name="ProfileDate"  />
    </property>
    <property name="ProfileStatus">
      <column name="ProfileStatus"  />
    </property>

    <property name="ProfileType">
      <column name="ProfileType"  />
    </property>

    

  </class>
</hibernate-mapping>