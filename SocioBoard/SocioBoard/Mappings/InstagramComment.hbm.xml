<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="SocioBoard.Domain"
      assembly="SocioBoard">

  <class name="InstagramComment" table="InstagramComment" lazy="false">
    <id name="Id" column="Id">
      <generator class="guid"/>
    </id>
    <property name="CommentId">
      <column name="CommentId" />
    </property>
    <property name="CommentDate">
      <column name="CommentDate"  />
    </property>
    <property name="EntryDate">
      <column name="EntryDate"  />
    </property>
    <property name="InstagramId">
      <column name="InstagramId"  />
    </property>
    <property name="UserId">
      <column name="UserId"  />
    </property>
    <property name="FeedId">
      <column name="FeedId"  />
    </property>
    <property name="Comment">
      <column name="Comment"  />
    </property>
    <property name="FromName">
      <column name="FromName"  />
    </property>
    <property name="FromProfilePic">
      <column name="FromProfilePic"  />
    </property>
  </class>
</hibernate-mapping>
