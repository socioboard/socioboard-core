<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="SocioBoard.Domain"
      assembly="SocioBoard">

  <class name="Package" table="Package" lazy="false">
    <id name="Id" column="Id">
      <generator class="guid"/>
    </id>

    <property name="PackageName">
      <column name="PackageName" />
    </property>
    <property name="Pricing">
      <column name="Pricing"  />
    </property>
    <property name="EntryDate">
      <column name="EntryDate"  />
    </property>
    <property name="Status">
      <column name="Status"  />
    </property>


    <property name="TotalProfiles">
      <column name="TotalProfiles"  />
    </property>

  </class>
</hibernate-mapping>
