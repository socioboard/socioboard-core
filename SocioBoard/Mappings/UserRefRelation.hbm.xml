<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="SocioBoard.Domain"
      assembly="SocioBoard">

  <class name="UserRefRelation" table="UserRefRelation" lazy="false">
    <id name="Id" column="Id">
      <generator class="guid"/>
    </id>
    <property name="ReferenceUserId">
      <column name="ReferenceUserId" />
    </property>
    <property name="RefereeUserId">
      <column name="RefereeUserId" />
    </property>
    <property name="ReferenceUserEmail">
      <column name="ReferenceUserEmail"  />
    </property>
    <property name="RefereeUserEmail">
      <column name="RefereeUserEmail"  />
    </property>
    <property name="EntryDate">
      <column name="EntryDate"  />
    </property>

    <property name="Status">
      <column name="Status"  />
    </property>
    
  

  </class>
</hibernate-mapping>
