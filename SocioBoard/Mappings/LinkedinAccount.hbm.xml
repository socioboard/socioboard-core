<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="SocioBoard.Domain"
      assembly="SocioBoard">

  <class name="LinkedInAccount" table="LinkedInAccount" lazy="false">
    <id name="Id" column="Id" >
      <generator class="guid"/>
    </id>
    <property name="LinkedinUserId">
      <column name="LinkedinUserId" />
    </property>
    <property name="LinkedinUserName">
      <column name="LinkedinUserName"  />
    </property>
    <property name="OAuthToken">
      <column name="OAuthToken"  />
    </property>
    <property name="OAuthSecret">
      <column name="OAuthSecret"  />
    </property>
    <property name="UserId">
      <column name="UserId" />
    </property>
    
<property name="ProfileImageUrl">
    <column name="ProfileImageUrl" />
  </property>
    <property name="OAuthVerifier">
      <column name="OAuthVerifier"  />
    </property>
    <property name="ProfileUrl">
      <column name="ProfileUrl"  />
    </property>
    <property name="EmailId">
      <column name="EmailId"  />
    </property>
    <property name="IsActive">
      <column name="IsActive"  />
    </property>
    <property name="Connections">
      <column name="Connections"  />
    </property>
  </class>
</hibernate-mapping>