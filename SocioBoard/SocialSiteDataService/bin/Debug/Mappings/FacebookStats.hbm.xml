<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="SocioBoard.Domain"
      assembly="SocioBoard">

  <class name="FacebookStats" table="FacebookStats" lazy="false">
    <id name="Id" column="Id" >
      <generator class="guid"/>
    </id>
    <property name="FbUserId">
      <column name="FbUserId" />
    </property>
    <property name="UserId">
      <column name="UserId"  />
    </property>
    <property name="MaleCount">
      <column name="MaleCount"  />
    </property>
    <property name="FemaleCount">
      <column name="FemaleCount"  />
    </property>
    <property name="ReachCount">
      <column name="ReachCount"  />
    </property>
    <property name="PeopleTalkingCount">
      <column name="PeopleTalkingCount"  />
    </property>
    <property name="LikeCount">
      <column name="LikeCount"  />
    </property>
    <property name="CommentCount">
      <column name="CommentCount"  />
    </property>
    <property name="ShareCount">
      <column name="ShareCount"/>
    </property>
    <property name="FanCount">
      <column name="FanCount" />
    </property>
    <property name="EntryDate">
      <column name="EntryDate" />
    </property>
  </class>
</hibernate-mapping>