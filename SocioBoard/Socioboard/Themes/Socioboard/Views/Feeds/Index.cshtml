    function LikeUnlikeInstagramPost(LikeCount, IsLike, FeedId, InstagramId) {
        debugger;
        var fd = new FormData();
        fd.append("LikeCount", LikeCount);
        fd.append("IsLike", IsLike);
        fd.append("FeedId", FeedId);
        fd.append("InstagramId", InstagramId);

        $.ajax({

            type: 'POST',
            url: '../Feeds/LikeUnlikeInstagramPost',
            data: fd,
            processData: false,
            contentType: false,
            dataType: "html",
            success: function (msg) {
                if (msg == 'unlike') {

                    //var count = +($("#" + FeedId + "_count").html()) + +1;
                    var count = +LikeCount + +1;
                    $("#count_" + FeedId).html(count);
                    //$("#" + FeedId + "_count").html(count);
                    $("#" + FeedId).attr("title", "unlike");
                    $("#" + FeedId).removeClass().addClass('fa fa-heart fa-fw')
                }
                else {
                    //var count = +($("#" + FeedId + "_count").html()) - +1;
                    var count = +LikeCount - +1;
                    $("#count_" + FeedId).html(count);
                    $("#" + FeedId).attr("title", "like");
                    $("#" + FeedId).removeClass().addClass('fa fa-heart-o fa-fw')
                }

            },
            async: false
        });

    }

    $(document).ready(function () {
        loadfeesmenu();
        $('#side-menu a').removeClass('active');
