@using Domain.Socioboard.Domain;
<div id="page-wrapper">
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">@GlobusMultiLanguageLib.Resources.Admin_UserDetails_LoadDeletedUsers_DeleteUsers</h1>
        </div>

        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    @GlobusMultiLanguageLib.Resources.Admin_UserDetails_LoadDeletedUsers_DeleteUsersTable
                </div>
                <!-- /.panel-heading -->
                <div class="panel-body">
                    <div class="row"></div>
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered table-hover" cellspacing="0" width="100%" id="ManageUserDataTable">
                            <thead>
                                <tr>
                                    <th>@GlobusMultiLanguageLib.Resources.Admin_UserDetails_LoadDeletedUsers_Name</th>
                                    <th>@GlobusMultiLanguageLib.Resources.Admin_UserDetails_LoadDeletedUsers_AccountType</th>
                                    <th>@GlobusMultiLanguageLib.Resources.Admin_UserDetails_LoadDeletedUsers_CreateDate</th>
                                    <th>@GlobusMultiLanguageLib.Resources.Admin_UserDetails_LoadDeletedUsers_EmailId</th>
                                    <th>@GlobusMultiLanguageLib.Resources.Admin_UserDetails_LoadDeletedUsers_UsersStatus</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (User item in (List<User>)Model)
                                {
        
                                    <tr class="odd gradeX">
                                        @*<td onclick="EditUserDetails('@item.Id');"><i class="fa fa-pencil fa-fw" style="cursor: pointer"></i></td>
                                        <td onclick="DeleteUserDetails('@item.Id');"><i class="fa fa-trash fa-fw" style="cursor: pointer"></i></td>*@
                                        <td>@item.UserName</td>
                                        <td>@item.AccountType</td>
                                        <td>@item.CreateDate</td>
                                        <td>@item.EmailId</td>
                                        <td>@GlobusMultiLanguageLib.Resources.Admin_UserDetails_LoadDeletedUsers_Deleted</td>
                                    </tr>
                                }
                            </tbody>

                        </table>
                    </div>
                    <!-- /.table-responsive -->
                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-lg-12 -->

    </div>
    <!-- /.row -->


</div>
