<div id="page-wrapper">
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">@GlobusMultiLanguageLib.Resources.Admin_Services_SchedulingDetails_UsersSchedulingDetails</h1>
        </div>

        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    @GlobusMultiLanguageLib.Resources.Admin_Services_SchedulingDetails_UsersSchedulingDetailsTable
                </div>
                <!-- /.panel-heading -->
                <div class="panel-body">
                    <div class="row"></div>
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered table-hover" cellspacing="0" width="100%" id="SchedulingDetailsDataTable">
