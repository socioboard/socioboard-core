<script src="~/Themes/Socioboard/Contents/js/jquery-1.10.2.min.js"></script>
<script type="text/javascript">

    function bindmanageServices() {
        $('#SchedulingDetailsDataTable').html('<center><img src="../Themes/Socioboard/Contents/img/admin/loading.gif" /></center>');
        $.ajax({
            type: 'POST',
            url: '../Services/LoadShedulingDetails',
            success: function (msg) {
                //alert(msg);
                $('#SchedulingDetailsDataTable').html(msg);
                $('#SchedulingDetailsDataTable').dataTable();
            },
        });
    }

   

    $(document).ready(function () {
        bindmanageServices();
    });
</script>
