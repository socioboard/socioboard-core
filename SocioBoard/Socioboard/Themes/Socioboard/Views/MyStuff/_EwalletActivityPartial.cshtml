                                            float affiliateamount = 0;
                                            List<Affiliates> lst = SBUtils.GetAffiliatesData(item.SenderUserId, item.FriendUserId);
                                            int months = (DateTime.Now.Month - item.AcceptInvitationDate.Month);
                                            foreach (var lstitem in lst)
