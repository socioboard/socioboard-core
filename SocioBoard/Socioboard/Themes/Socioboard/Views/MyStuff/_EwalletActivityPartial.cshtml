                                                affiliateamount +=float.Parse(lstitem.Amount);
                                            }
                                            <tr class="odd gradeX">
                                                <td>@item.FriendEmail</td>
                                                <td>@item.AcceptInvitationDate</td>
                                                <td>@months.ToString()</td>
                                                <td class="center">@affiliateamount.ToString()</td>
