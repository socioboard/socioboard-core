<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="SocioBoard.Domain"
      assembly="SocioBoard">

  <class name="ScheduledMessage" table="ScheduledMessage" lazy="false">
    <id name="Id" column="Id" >
      <generator class="guid"/>
    </id>
    <property name="ShareMessage">
      <column name="ShareMessage" />
    </property>
    <property name="ClientTime">
      <column name="ClientTime"  />
    </property>
    <property name="ScheduleTime">
      <column name="ScheduleTime"  />
    </property>
    <property name="CreateTime">
      <column name="CreateTime"  />
    </property>


    <property name="UserId">
      <column name="UserId"  />
    </property>

    <property name="ProfileId">
      <column name="ProfileId"  />
    </property>

    <property name="ProfileType">
      <column name="ProfileType"  />
    </property>

    <property name="PicUrl">
      <column name="PicUrl"  />
    </property>

    <property name="Status">
      <column name="Status"  />
    </property>


  </class>
</hibernate-mapping>