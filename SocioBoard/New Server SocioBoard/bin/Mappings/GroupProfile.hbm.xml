<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="SocioBoard.Domain"
      assembly="SocioBoard">

  <class name="GroupProfile" table="GroupProfile" lazy="false">
    <id name="Id" column="Id" >
      <generator class="guid"/>
    </id>
    <property name="GroupId">
      <column name="GroupId" />
    </property>
    <property name="ProfileId">
      <column name="ProfileId"  />
    </property>
    <property name="GroupOwnerId">
      <column name="GroupOwnerId"  />
    </property>
    <property name="ProfileType">
      <column name="ProfileType"  />
    </property>
    <property name="EntryDate">
      <column name="EntryDate"  />
    </property>
  </class>
</hibernate-mapping>